                android:textColor="#ff000000"
                app:layout_flexGrow="1" />

            <TextView
                android:id="@+id/prdnwtxtspin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/edtxtcomb_resoc"
                android:gravity="left"
                android:padding="10dp"
                android:text="@string/gen_stra"
                android:textColor="#ff000000"
                app:layout_flexBasisPercent="65%"
                app:layout_flexGrow="1" />
        </com.google.android.flexbox.FlexboxLayout>

        <com.google.android.flexbox.FlexboxLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="26dp"
            app:alignContent="flex_start"
            app:alignItems="flex_start"
            app:flexDirection="row"
            app:flexWrap="wrap">

            <TextView
                android:id="@+id/prdnwtxtb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/txview_resoc"
                android:gravity="left"
                android:padding="10dp"
                android:text="@string/prdct_strg"
                android:textColor="#ff000000"
                app:layout_flexGrow="1" />

            <EditText
                android:id="@+id/prdnwedtxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="-3dp"
                android:background="@drawable/edtxtcomb_resoc"
                android:padding="10dp"
                android:textColor="#ff000000"
                app:layout_flexBasisPercent="65%"
                app:layout_flexGrow="1" />
        </com.google.android.flexbox.FlexboxLayout>

        <com.google.android.flexbox.FlexboxLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            app:alignContent="flex_start"
            app:alignItems="flex_start"
            app:flexDirection="row"
            app:flexWrap="wrap"
            app:justifyContent="flex_start">

            <TextView
                android:id="@+id/prdnwtxtd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/txview_resoc"
